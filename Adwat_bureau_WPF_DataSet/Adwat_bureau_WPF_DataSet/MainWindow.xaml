<Window x:Class="Adwat_bureau_WPF_DataSet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Adwat_bureau_WPF_DataSet"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.14*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden" Grid.Row="1" Grid.ColumnSpan="2" />
        <Button x:Name="PageClients_Button" Content="Таблиц Клиентов" Grid.Row="0" Grid.Column="0" Click="PageClients_Button_Click"/>
        <Button x:Name="PageLawyer_Button" Content="Таблиц Адвокатов" Grid.Row="0" Grid.Column="1" Click="PageLawyer_Button_Click"/>

    </Grid>
</Window>
